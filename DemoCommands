   ls -clear
   sudo su - //Command to move to root access
   aws s3 ls // command to list all buckets created in S3
   aws configure // to configure AWS CLI
   aws s3 ls 
   ls -la // List all files and folders
   cd .aws // move to new folder
   ls -la
   cat credentials // print contents of file
   rm -rf credentials // delete file
   ls -la
   cd .. // move to previous folder
   cd..
   aws s3 ls
   history // show history of commands used
